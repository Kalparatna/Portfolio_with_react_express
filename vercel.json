{
  "version": 2,
  "builds": [
    {
      "src": "Vite-React-Portfolio-main/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Vite-React-Portfolio-main/$1"
    }
  ]
}
